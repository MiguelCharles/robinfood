<div class="container padded-top">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= simple_form_for(@shop) do |s| %>
        <%= s.input :address %>
        <%= s.input :zip_code %>
        <%= s.input :city %>
        <%= s.input :country, priority: [ 'FR' ] %>
        <%= s.input :category %>
        <%= s.input :description %>
        <%= s.input :phone_number %>
        <%= s.input :VAT_number %>
        <%= s.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyDANjx3bosEtIyzJaoWs50Wnt6nt_1rmxU"></script>
<% end %>
