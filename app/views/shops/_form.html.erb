<div class="container top">
<div class="row">


<div class="container padded-top">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= simple_form_for(shop) do |s| %>
      <div class="form-inputs">
        <%= s.input :name_of_the_store %>
        <%= s.input :address %>
        <%= s.input :zip_code %>
        <%= s.input :city %>
        <%= s.input :country, priority: [ 'FR' ] %>
        <%= s.input :category %>
        <%= s.input :description %>
        <%= s.input :phone_number %>
        <%= s.input :VAT_number %>
        </div>
        <%= s.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=<%= ENV['GOOGLE_API_BROWSER_KEY'] %>"></script>
<% end %>
</div>
</div>
